package arrange;
import java.util.Scanner;
public class notebook{
    public static void main(String[] args){
        
    	int day = 3;
    	if (day == 1);{
    	switch(day) {
    
    	case 3:
    		System.out.println("hello");
    	break;
    	
    	case 2:
    		System.out.println("hi");
    	break;
    	
    	case 1:
    		System.out.println("wonho");
    	break;
    	
    	case 4:
    		System.out.println("bye");
    	break;
    	
    	case 5:
    		System.out.println("goodbye");
    	break;
    	}
    	}
    	
    
    }
}