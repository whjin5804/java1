package arrange;
import java.util.Scanner;
public class notebook{
    public static void main(String[] args){
        
    	int i = 0;
    	while ( i < 4 ) {
    		System.out.println(i);
    		i++;
    	}
    	
    	System.out.println("hello");
    	int j = 1;
    	do {
    		System.out.println(j);
    		j++;
    	}
    	while( j < 5);
    }
}