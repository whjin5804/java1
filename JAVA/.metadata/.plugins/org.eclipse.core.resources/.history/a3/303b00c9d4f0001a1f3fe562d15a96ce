import org.opentutorials.iot.Elevator;
import org.opentutorials.iot.Lighting;
import org.opentutorials.iot.Security;


public class GavaGoHome {

	public static void main(String[] args) {

		// Elevator Call
		Elevator myElevator = new Elevator("java APT 501");
		myElevator.callForUp(1);
		
		// Security Off
		Security mySecurity = new Security("java APT 501")
		mySecurity.off();
		
		// Light On
		Lighting hallLamp = new Lighting("java APT 501 / Hall Lamp");
		hallLamp.on();

		Lighting floorLamp = new Lighting("java APT 501 / floor Lamp");
		floorLamp.on();
	}

}
